<section class="wt-haslayout wt-dbsectionspace">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
            <div class="wt-dashboardbox">
                <div class="wt-dashboardboxtitle">
                    <h2>All Jobs</h2>
                </div>
                <div class="wt-dashboardboxcontent wt-jobdetailsholder">
                    <div class="wt-completejobholder">
                        <div class="wt-tabscontenttitle">
                            <h2>Jobs</h2>
                        </div>
                        <div class="wt-managejobcontent">
                            @foreach(var i in Model)
                                {



                            <div class="wt-userlistinghold wt-featured wt-userlistingvtwo">

                                @if (User.IsConsumer())
                                {
                                    <div class="wt-userlistingcontent wt-userlistingcontentvtwo">
                                        <div class="wt-contenthead">
                                            <div class="wt-title">
                                                <a href="#">
                                                    <i class="fa fa-check-circle"></i> @i.ProviderName
                                                </a>
                                                <h2>@i.instruction</h2>
                                                <p>@i.ProviderAddress</p>
                                            </div>
                                            <ul class="wt-saveitem-breadcrumb wt-userlisting-breadcrumb">
                                                <li><span class="wt-dashboraddoller"> @i.ProviderEmail</span></li>

                                                <li> <span> @(i.completed?"Completed":"Pendeing") Task</span></li>
                                                <li><span class="wt-dashboradclock"><i class="far fa-clock"></i> Date: @i.OnDate.ToShortDateString()</span></li>
                                            </ul>
                                        </div>
                                        <div class="wt-rightarea">


                                            <div class="wt-hireduserstatus">
                                                @*<h4>Hired</h4><span>Terrence Tynan</span>*@
                                                <ul class="wt-hireduserimgs">
                                                    <li><figure><img gravatar-email="@i.ProviderAddress" alt="img description" /></figure></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                }
                                @if (User.IsServiceProvider())
                                {
                                    <div class="wt-userlistingcontent wt-userlistingcontentvtwo">
                                        <div class="wt-contenthead">
                                            <div class="wt-title">
                                                <a href="#">
                                                    <i class="fa fa-check-circle"></i> @i.ConsumerName
                                                </a>
                                                <h2>@i.instruction</h2>
                                                <p>@i.ConsumerAddress</p>
                                            </div>
                                            <ul class="wt-saveitem-breadcrumb wt-userlisting-breadcrumb">
                                                <li><span class="wt-dashboraddoller">@i.ConsumerEmail</span></li>
                                                <li> <span> @(i.completed?"Completed":"Pendeing") Task</span></li>
                                                
                                                <li><span class="wt-dashboradclock"><i class="far fa-clock"></i> Date: @i.OnDate.ToShortDateString()</span></li>
                                            </ul>
                                        </div>
                                        <div class="wt-rightarea">

                                           
                                            <div class="wt-hireduserstatus">
                                                @*<h4>Hired</h4><span>Terrence Tynan</span>*@
                                                <ul class="wt-hireduserimgs">
                                                    <li><figure><img gravatar-email="@i.ConsumerEmail" alt="img description" /></figure></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                }

                           <a asp-action="bookingdetails" asp-controller="home" asp-route-id="@i.booking_id" class="btn btn-success">Details</a> 

                            </div>
                            
                                }
                            
                        </div>
                    </div>
                </div>
                <nav class="wt-pagination wt-savepagination">
                    <ul>
                        <li class="wt-prevpage"><a href="javascrip:void(0);"><i class="lnr lnr-chevron-left"></i></a></li>
                        <li><a href="javascrip:void(0);">1</a></li>
                        <li><a href="javascrip:void(0);">2</a></li>
                        <li><a href="javascrip:void(0);">3</a></li>
                        <li><a href="javascrip:void(0);">4</a></li>
                        <li><a href="javascrip:void(0);">...</a></li>
                        <li><a href="javascrip:void(0);">50</a></li>
                        <li class="wt-nextpage"><a href="javascrip:void(0);"><i class="lnr lnr-chevron-right"></i></a></li>
                    </ul>
                </nav>
            </div>
        </div>
       
    </div>
</section>


